<h1>Listing Child Sub Categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Descriptions</th>
      <th>Slug</th>
      <th>Category</th>
      <th>Subcategory</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @child_sub_categories.each do |child_sub_category| %>
      <tr>
        <td><%= child_sub_category.name %></td>
        <td><%= child_sub_category.descriptions %></td>
        <td><%= child_sub_category.slug %></td>
        <td><%= child_sub_category.category %></td>
        <td><%= child_sub_category.subcategory %></td>
        <td><%= link_to 'Show', child_sub_category %></td>
        <td><%= link_to 'Edit', edit_child_sub_category_path(child_sub_category) %></td>
        <td><%= link_to 'Destroy', child_sub_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

